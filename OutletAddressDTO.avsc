    },
    {
      "name": "number",
      "type": ["null", "string"],
      "doc": "Номер дома.",
      "default": null
    },
    {
      "name": "building",
      "type": ["null", "string"],
      "doc": "Номер строения.",
      "default": null
    },
    {
      "name": "estate",
