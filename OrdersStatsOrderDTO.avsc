      "default": null
    },
    {
      "name": "paymentType",
      "type": ["null", "ympa_avro_schema.OrdersStatsOrderPaymentType"],
      "doc": "",
      "default": null
    },
    {
      "name": "fake",
      "type": ["null", "boolean"],
