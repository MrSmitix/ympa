"$schema"   = "http://cyaninc.com/json-schemas/tosca-lite-v1/definition-module#"
title       = "ympa_python_blueplanet_server OrderDeliveryDTO"
package     = ympa_python_blueplanet_server
version     = "1.0"
description = "Defines a OrderDeliveryDTO"
authors     = ["F. Bar (foo@bar.baz)"]

imports {
  Root = tosca.resourceTypes.Root
  OrderCourierDTO = ympa_python_blueplanet_server.resourceTypes.OrderCourierDTO
  OrderDeliveryAddressDTO = ympa_python_blueplanet_server.resourceTypes.OrderDeliveryAddressDTO
  OrderDeliveryDatesDTO = ympa_python_blueplanet_server.resourceTypes.OrderDeliveryDatesDTO
  OrderDeliveryDispatchType = ympa_python_blueplanet_server.resourceTypes.OrderDeliveryDispatchType
