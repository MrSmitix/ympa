    properties {
        total {
          type =  integer
          description = "Сколько всего найдено элементов."
          optional = false
        }
        _from {
          type =  integer
          description = "Начальный номер найденного элемента на странице."
          optional = false
        }
        to {
          type =  integer
          description = "Конечный номер найденного элемента на странице."
