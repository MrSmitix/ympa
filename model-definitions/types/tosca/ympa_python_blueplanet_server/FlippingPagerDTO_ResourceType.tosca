        current_page {
          type =  integer
          description = "Текущая страница."
          optional = false
        }
        pages_count {
          type =  integer
          description = "Общее количество страниц."
          optional = false
        }
        page_size {
          type =  integer
          description = "Размер страницы."
          optional = false
        }
    }
  }
}

serviceTemplates {

