package     = ympa_python_blueplanet_server
version     = "1.0"
description = "Defines a FlippingPagerDTO"
authors     = ["F. Bar (foo@bar.baz)"]

imports {
  Root = tosca.resourceTypes.Root
}
resourceTypes {

  FlippingPagerDTO {
    title = FlippingPagerDTO
    description = FlippingPagerDTO
